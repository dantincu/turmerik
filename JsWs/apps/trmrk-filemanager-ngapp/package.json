{
  "name": "trmrk-filemanager-ngapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:dev": "scripts\\copy-app-config.bat dev && ng serve --configuration=development --host=0.0.0.0 --port=4201 --ssl=true --ssl-key=./sslcert/key.pem --ssl-cert=./sslcert/cert.pem",
    "start:prod": "scripts\\copy-app-config.bat prod && ng serve --configuration=production --host=0.0.0.0 --port=4201 --ssl=true --ssl-key=./sslcert/key.pem --ssl-cert=./sslcert/cert.pem",
    "build:dev": "scripts\\copy-app-config.bat dev && ng build --configuration=development",
    "build:prod": "scripts\\copy-app-config.bat prod && ng build --configuration=production"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^21.0.2",
    "@angular/platform-browser": "^21.0.3",
    "@angular/router": "^21.0.3",
    "big.js": "^7.0.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@types/big.js": "^6.2.2",
    "@types/jasmine": "~5.1.13",
    "@types/node": "^24.10.1",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3"
  }
}
