{
  "FileNameMaxLength": 100,
  "ThrowIfAnyItemAlreadyExists": true,
  "CreatePdfFile": true,
  "ArgOpts": {
    "PrintHelpMessage": "h",
    "WorkDir": "wd",
    "InteractiveMode": "i",
    "OpenMdFile": "o",
    "SkipMdFileCreation": "skmd",
    "SkipPdfFileCreation": "skpdf",
    "SkipCurrentNode": "skcrnt",
    "SkipUntilPath": "skup",
    "CreatePdfFile": "pdf",
    "DirNameTpl": "t",
    "CreateNote": "n",
    "CreateNoteSection": "s",
    "CreateNoteBook": "nb",
    "CreateNoteInternalsDir": "ni",
    "CreateNoteFilesDir": "nf",
    "ConvertToNoteSections": "cts",
    "ConvertToNoteItems": "ctn",
    "Url": "url",
    "Uri": "uri",
    "ShowLastCreatedFirst": "slf",
    "ShowOtherDirNames": "so",
    "HcyChildNode": "+",
    "HcyParentNode": "-",
    "HcySibblingNode": "=",
    "Macro": "m",
    "Title": "t",
    "RecursiveMatchingDirNames": "rec"
  },
  "DirNames": {
    "DefaultJoinStr": "-",
    "DirNamesTplMap": {
      "f": {
        "DirNameTpl": "[note-files]",
        "MdFileNameTemplate": ""
      },
      "i": {
        "DirNameTpl": "[note-internals]",
        "MdFileNameTemplate": ""
      },
      "b": {
        "DirNameTpl": "[note-book]",
        "MdFileNameTemplate": ""
      }
    },
    "MacrosMap": {
      "|m|": "$misc$",
      "|a|": "$articles$",
      "|p|": "$persons$",
      "|nf|": "$infamous$",
      "|pp|": "$public-persons$",
      "|hf|": "$historical-figures$",
      "|tc|": "$terms-and-concepts$",
      "|ps|": "$products-and-services$",
      "|egna|": "$environment-geography-nature-and-agriculture$",
      "|pl|": "$places$",
      "|gr|": "$geographical-regions$",
      "|plgr|": "$places-and-geographical-regions$",
      "|plyr|": "$players$",
      "|athl|": "$athletes$",
      "|fplyr|": "$former-players$",
      "|fathl|": "$former-athletes$"
    }
  },
  "FileNames": {
    "MdFileName": "[note].md",
    "MdFileNamePfx": "0-",
    "PrependTitleToNoteMdFileName": true,
    "KeepFileName": ".keep"
  },
  "FileContents": {
    "KeepFileContentsTemplate": "{0}",
    "KeepFileContainsNoteJson": true,
    "MdFileContentsTemplate": "# {0}  \n\n",
    "MdFileContentSectionTemplate": "{0}  \n\n"
  },
  "Macros": {
    "Map": {
      "m": [ ":s", "$misc$", ":=" ],
      "a": [ ":s", "$articles$", ":=" ],
      "p": [ ":s", "$persons$", ":=" ],
      "nf": [ ":s", "$infamous$", ":=" ],
      "pp": [ ":s", "$public-persons$", ":=" ],
      "hf": [ ":s", "$historical-figures$", ":=" ],
      "tc": [ ":s", "$terms-and-concepts$", ":=" ],
      "ps": [ ":s", "$products-and services$", ":=" ],
      "egna": [ ":s", "$environment-geography-nature-and-agriculture$", ":=" ],
      "pl": [ ":s", "$places$", ":=" ],
      "gr": [ ":s", "$geographical-regions$", ":=" ],
      "plgr": [ ":s", "$places-and-geographical-regions$", ":=" ],
      "plyr": [ ":s", "$players$", ":=" ],
      "athl": [ ":s", "$athletes$", ":=" ],
      "fplyr": [ ":s", "$former-players$", ":=" ],
      "fathl": [ ":s", "$former-athletes$", ":=" ]
    }
  }
}