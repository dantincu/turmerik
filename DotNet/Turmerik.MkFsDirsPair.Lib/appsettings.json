    "SerializeToJson":  true,
    "NoteDirPairsIntegrityCheck": "None",
    "Prefixes": {
      "NoteBook": "b",
      "NoteFiles": "f",
      "NoteInternals": "i",
      "Note": "n"
    },
    "ArgOpts": {
    },
    "DirNames": {
      "NoteBook": "[note-book]",
      "NoteFiles": "[note-files]",
      "NoteInternals": "[note-internals]",
