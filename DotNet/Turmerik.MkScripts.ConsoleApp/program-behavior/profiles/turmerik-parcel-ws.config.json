{
  "ProfileName": "turmerik-parcel-ws",
  "RelDirPaths": {
    "DirPath": "|$TURMERIK_REPO_DIR|\\ParcelWs"
  },
  "Sections": [
    {
      "SectionName": "workspace-lvl",
      "RelDirPaths": {
        "DirPath": "scripts\\all"
      },
      "FileGroups": [
        {
          "DefaultContentSpecs": {
            "Args": [
              [ "..\\..\\libs\\trmrk" ],
              [ "..\\..\\libs\\trmrk-axios" ],
              [ "..\\..\\libs\\trmrk-blazor" ],
              [ "..\\..\\libs\\trmrk-browser" ],
              [ "..\\..\\libs\\trmrk-react" ],
              [ "..\\..\\libs\\trmrk-lithtml" ],
              [ "..\\..\\libs\\trmrk-mkscripts-parcelws-sync-behavior" ],
              [ "..\\..\\libs\\trmrk-mkscripts-parcelws-sync-behavior-testapp" ],
              [ "..\\..\\libs\\trmrk-text-transform-behavior" ],
              [ "..\\..\\libs\\trmrk-text-transform-behavior-testapp" ],
              [ "..\\..\\libs\\trmrk-text-transform-defaultbehavior" ],
              [ "..\\..\\libs\\trmrk-text-transform-mybehavior" ],
              [ "..\\..\\apps\\trmrk-react-testapp" ],
              [ "..\\..\\apps\\trmrk-notes-reactapp" ],
              [ "..\\..\\apps\\trmrk-notes-lithtmlapp" ]
            ],
            "ArgFilters": {
              "libs": {
                "Regexes": [ "^\\.\\.\\\\\\.\\.\\\\libs\\.*" ]
              },
              "apps": {
                "Regexes": [ "^\\.\\.\\\\\\.\\.\\\\apps\\.*" ]
              }
            },
            "ArgsSetTpl": "\"{0}\"",
            "ArgsSetsJoinTpl": ",\n"
          },
          "Files": [
            {
              "FileRelPath": "upd-pcks.bat",
              "FileRelPathsMap": {
                "libs": "upd-libs-pcks.bat",
                "apps": "upd-apps-pcks.bat"
              },
              "ContentSpecs": [
                {
                  "TemplateLines": [
                    "@echo off",
                    "",
                    "for %%a in ({0}) do (",
                    "  cd %%a",
                    "  .\\_\\upd-pcks.bat",
                    ")",
                    "",
                    "@echo on"
                  ]
                }
              ]
            },
            {
              "FileRelPath": "rm.bat",
              "FileRelPathsMap": {
                "libs": "rm-libs.bat",
                "apps": "rm-apps.bat"
              },
              "ContentSpecs": [
                {
                  "TemplateLines": [
                    "@echo off",
                    "",
                    "for %%a in ({0}) do (",
                    "  cd %%a",
                    "  .\\_\\rm.bat",
                    ")",
                    "",
                    "@echo on"
                  ]
                }
              ]
            },
            {
              "FileRelPath": "rm-pcks.bat",
              "FileRelPathsMap": {
                "libs": "rm-libs-pcks.bat",
                "apps": "rm-apps-pcks.bat"
              },
              "ContentSpecs": [
                {
                  "TemplateLines": [
                    "@echo off",
                    "",
                    "for %%a in ({0}) do (",
                    "  cd %%a",
                    "  .\\_\\rm-pcks.bat",
                    ")",
                    "",
                    "@echo on"
                  ]
                }
              ]
            },
            {
              "FileRelPath": "rm-nd_mods.bat",
              "FileRelPathsMap": {
                "libs": "rm-libs-nd_mods.bat",
                "apps": "rm-apps-nd_mods.bat"
              },
              "ContentSpecs": [
                {
                  "TemplateLines": [
                    "@echo off",
                    "",
                    "for %%a in ({0}) do (",
                    "  cd %%a",
                    "  .\\_\\rm-nd_mods.bat",
                    ")",
                    "",
                    "@echo on"
                  ]
                }
              ]
            },
            {
              "FileRelPath": "rm-ch_dst.bat",
              "FileRelPathsMap": {
                "libs": "rm-libs-ch_dst.bat",
                "apps": "rm-apps-ch_dst.bat"
              },
              "ContentSpecs": [
                {
                  "TemplateLines": [
                    "@echo off",
                    "",
                    "for %%a in ({0}) do (",
                    "  cd %%a",
                    "  .\\_\\rm-ch_dst.bat",
                    ")",
                    "@echo on"
                  ]
                }
              ]
            },
            {
              "FileRelPath": "rebuild.bat",
              "FileRelPathsMap": {
                "libs": "rebuild-libs.bat",
                "apps": "rebuild-apps.bat"
              },
              "ContentSpecs": [
                {
                  "TemplateLines": [
                    "@echo off",
                    "",
                    "for %%a in ({0}) do (",
                    "  cd %%a",
                    "  .\\_\\rm-ch_dst.bat",
                    "  npm run build",
                    ")",
                    "@echo on"
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "SectionName": "package-lvl",
      "RelDirPaths": {
        "DirPathsArr": [
          "libs\\trmrk\\_",
          "libs\\trmrk-axios\\_",
          "libs\\trmrk-blazor\\_",
          "libs\\trmrk-browser\\_",
          "libs\\trmrk-react\\_",
          "libs\\trmrk-lithtml\\_",
          "libs\\trmrk-mkscripts-parcelws-sync-behavior\\_",
          "libs\\trmrk-mkscripts-parcelws-sync-behavior-testapp\\_",
          "libs\\trmrk-text-transform-behavior\\_",
          "libs\\trmrk-text-transform-behavior-testapp\\_",
          "libs\\trmrk-text-transform-defaultbehavior\\_",
          "libs\\trmrk-text-transform-mybehavior\\_",
          "apps\\trmrk-react-testapp\\_",
          "apps\\trmrk-notes-reactapp\\_",
          "apps\\trmrk-notes-lithtmlapp\\_"
        ],
        "Filters": {
          "libs": {
            "Regex": "^libs\\\\.*"
          },
          "apps": {
            "Regex": "^apps\\\\.*"
          }
        }
      },
      "FileGroups": [
        {
          "Files": [
            {
              "FileRelPath": "upd-pcks.bat",
              "TextContentLines": [
                "CALL ncu -u",
                "CALL npm i"
              ]
            },
            {
              "FileRelPath": "rm.bat",
              "TextContentLines": [
                "CALL rimraf .\\.parcel-cache",
                "CALL rimraf .\\dist",
                "CALL rimraf .\\node_modules",
                "CALL del .\\package-lock.json"
              ]
            },
            {
              "FileRelPath": "rm-pcks.bat",
              "TextContentLines": [
                "CALL rimraf .\\node_modules",
                "CALL del .\\package-lock.json"
              ]
            },
            {
              "FileRelPath": "rm-nd_mods.bat",
              "TextContentLines": [
                "CALL rimraf .\\node_modules"
              ]
            },
            {
              "FileRelPath": "rm-ch_dst.bat",
              "TextContentLines": [
                "CALL rimraf .\\.parcel-cache",
                "CALL rimraf .\\dist"
              ]
            }
          ]
        }
      ]
    }
  ]
