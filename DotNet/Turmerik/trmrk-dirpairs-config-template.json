{
  "FileNameMaxLength": 100,
  "ThrowIfAnyItemAlreadyExists": true,
  "TrmrkGuidInputName": "trmrk_guid",
  "ArgOpts": {
    "PrintHelpMessage": "h",
    "WorkDir": "wd",
    "InteractiveMode": "i",
    "OpenMdFile": "o",
    "SkipMdFileCreation": "sk",
    "DirNameTpl": "t",
    "CreateNote": "n",
    "CreateNoteSection": "s",
    "CreateNoteBook": "nb",
    "CreateNoteInternalsDir": "ni",
    "CreateNoteFilesDir": "nf",
    "ConvertToNoteSections": "cts",
    "ConvertToNoteItems": "ctn",
    "Url": "url",
    "Uri": "uri",
    "ShowLastCreatedFirst": "slf",
    "ShowOtherDirNames": "so",
    "HcyChildNode": "+",
    "HcyParentNode": "-",
    "HcySibblingNode": "=",
    "Macro": "m"
  },
  "DirNames": {
    "DefaultJoinStr": "-",
    "DirNamesTplMap": {
      "f": {
        "DirNameTpl": "[note-files]",
        "MdFileNameTemplate": ""
      },
      "i": {
        "DirNameTpl": "[note-internals]",
        "MdFileNameTemplate": ""
      },
      "b": {
        "DirNameTpl": "[note-book]",
        "MdFileNameTemplate": ""
      }
    }
  },
  "FileNames": {
    "MdFileName": "[note].md",
    "PrependTitleToNoteMdFileName": true,
    "KeepFileName": ".keep"
  },
  "FileContents": {
    "KeepFileContentsTemplate": "{0}",
    "KeepFileContainsNoteJson": true,
    "MdFileContentsTemplate": "# {0}  \n\n",
    "MdFileContentSectionTemplate": "{0}  \n\n"
  },
  "Macros": {
    "Map": {
      "m": [ ":s", "Misc", ":=" ],
      "a": [ ":s", "Articles", ":=" ],
      "p": [ ":s", "Persons", ":=" ],
      "pp": [ ":s", "Public Persons", ":=" ],
      "hs": [ ":s", "Historical Figures", ":=" ],
      "tc": [ ":s", "Terms & Concepts", ":=" ],
      "ps": [ ":s", "Products & Services", ":=" ]
    }
  }
}