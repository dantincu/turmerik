{
  "Trmrk": {
    "FileNameMaxLength": 100,
    "Prefixes": {
      "NoteBook": "b",
      "NoteFiles": "f",
      "NoteInternals": "i",
      "Note": "n"
    },
    "ArgOpts": {
      "GroudIdx": "/g:"
    },
    "DirNames": {
      "NoteBook": "[note-book]",
      "NoteFiles": "[note-files]",
      "NoteInternals": "[note-internals]",
      "JoinStr": "-"
    },
    "FileNames": {
      "KeepFileName": ".keep",
      "NoteMdFileName": "[note].md",
      "NoteJsonFileName": "[note].json"
    },
    "FileContents": {
      "KeepFileContentsTemplate": "",
      "NoteFileContentsTemplate":  "# {0}  \n\n"
    }
  }
}