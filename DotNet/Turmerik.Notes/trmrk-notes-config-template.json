{
  "IsDevEnv": false,
  "RequiredClientVersion": 1,
  "ClientRedirectUrl": null,
  "NoteDirPairs": {
    "FileNameMaxLength": 100,
    "SerializeToJson": true,
    "AllowGetRequestsToPersistChanges": true,
    "TrmrkGuidInputName": "trmrk_guid",
    "ArgOpts": {
      "SrcNote": {
        "FullArg": "from-note",
        "ShortArg": "fn"
      },
      "SrcDirIdnf": {
        "FullArg": "from-dir-idnf",
        "ShortArg": "fid"
      },
      "SrcNoteIdx": {
        "FullArg": "from-note-idx",
        "ShortArg": "fnx"
      },
      "DestnNote": {
        "FullArg": "to-note",
        "ShortArg": "tn"
      },
      "DestnDirIdnf": {
        "FullArg": "to-dir-idnf",
        "ShortArg": "tid"
      },
      "DestnNoteIdx": {
        "FullArg": "to-note-idx",
        "ShortArg": "tnx"
      },
      "SortIdx": {
        "FullArg": "sort-idx",
        "ShortArg": "si"
      },
      "OpenMdFile": {
        "FullArg": "open-md-file",
        "ShortArg": "o"
      },
      "CreateNoteFilesDirsPair": {
        "FullArg": "note-files",
        "ShortArg": "f"
      },
      "CreateNoteInternalDirsPair": {
        "FullArg": "note-int",
        "ShortArg": "i"
      },
      "CommandsMap": {
        "Help": {
          "FullArg": "help",
          "ShortArg": "h"
        },
        "ListNotes": {
          "FullArg": "list-notes",
          "ShortArg": "ls"
        },
        "CreateNoteBook": {
          "FullArg": "create-note-book",
          "ShortArg": "bk"
        },
        "CreateNoteBookInternal": {
          "FullArg": "create-note-book-int",
          "ShortArg": "bki"
        },
        "CreateNote": {
          "FullArg": "create-note",
          "ShortArg": "mk"
        },
        "CreateNoteInternal": {
          "FullArg": "create-note-int",
          "ShortArg": "mki"
        },
        "CopyNotes": {
          "FullArg": "copy-notes",
          "ShortArg": "cp"
        },
        "DeleteNotes": {
          "FullArg": "delete-notes",
          "ShortArg": "dl"
        },
        "MoveNotes": {
          "FullArg": "move-notes",
          "ShortArg": "mv"
        },
        "RenameNote": {
          "FullArg": "rename-note",
          "ShortArg": "rn"
        },
        "NormalizeSortIndexes": {
          "FullArg": "norm-sort-idxes",
          "ShortArg": "normsi"
        },
        "NormalizeNoteIndexes": {
          "FullArg": "norm-note-idxes",
          "ShortArg": "normnx"
        }
      }
    },
    "DirNames": {
      "NoteBook": "[note-book]",
      "NoteFiles": "[note-files]",
      "NoteInternals": "[note-internals]",
      "NoteInternalsPfx": "0",
      "NoteItemsPfx": "",
      "JoinStr": "-"
    },
    "NoteDirNameIdxes": {
      "MinIdx": 101,
      "MaxIdx": 799,
      "IncIdx": true,
      "FillGapsByDefault": false,
      "IdxFmt": "D3"
    },
    "NoteInternalDirNameIdxes": {
      "MinIdx": 1,
      "MaxIdx": 99,
      "IncIdx": true,
      "FillGapsByDefault": true,
      "IdxFmt": "D1"
    },
    "FileNames": {
      "NoteBookJsonFileName": "[note-book].json",
      "NoteItemJsonFileName": "[note].json",
      "NoteItemMdFileName": "[note].md",
      "PrependTitleToNoteMdFileName": true,
      "KeepFileName": ".keep"
    },
    "FileContents": {
      "KeepFileContentsTemplate": "{0}",
      "NoteFileContentsTemplate": "<input type=\"hidden\" name=\"{2}\" value=\"{1}\" />\n\n# {0}  \n\n",
      "RequireTrmrkGuidInNoteJsonFile": true,
      "RequireTrmrkGuidInNoteMdFile": false
    }
  }
}
