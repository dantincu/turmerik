{
  "IsDevEnv": false,
  "RequiredClientVersion": 1,
  "ClientRedirectUrl": null,
  "NoteDirPairs": {
    "FileNameMaxLength": 100,
    // "SerializeToJson": true,
    "TrmrkGuidInputName": "trmrk_guid",
    "ArgOpts": {
      "SrcNote": "fn",
      "SrcDirIdnf": "fid",
      "SrcNoteIdx": "fnx",
      "DestnNote": "tn",
      "DestnDirIdnf": "tid",
      "DestnNoteIdx": "tnx",
      "SortIdx": "si",
      "OpenMdFile": "o",
      "CreateNoteFilesDirsPair": "f",
      "CreateNoteInternalDirsPair": "i",
      "CommandsMap": {
        "ListNotes": {
          "FullArgValue": "list-notes",
          "ShortArgValue": "ls"
        },
        "CreateNoteBook": {
          "FullArgValue": "create-note-book",
          "ShortArgValue": "bk"
        },
        "CreateNoteBookInternal": {
          "FullArgValue": "create-note-book-internal",
          "ShortArgValue": "bki"
        },
        "CreateNote": {
          "FullArgValue": "create-note",
          "ShortArgValue": "mk"
        },
        "CreateNoteInternal": {
          "FullArgValue": "create-note-internal",
          "ShortArgValue": "mki"
        },
        "CopyNotes": {
          "FullArgValue": "copy-notes",
          "ShortArgValue": "cp"
        },
        "DeleteNotes": {
          "FullArgValue": "delete-notes",
          "ShortArgValue": "dl"
        },
        "MoveNotes": {
          "FullArgValue": "move-notes",
          "ShortArgValue": "mv"
        },
        "RenameNote": {
          "FullArgValue": "rename-note",
          "ShortArgValue": "rn"
        },
        "NormalizeSortIndexes": {
          "FullArgValue": "norm-sort-idxes",
          "ShortArgValue": "normsi"
        },
        "NormalizeNoteIndexes": {
          "FullArgValue": "norm-note-idxes",
          "ShortArgValue": "normnx"
        }
      }
    },
    "DirNames": {
      "NoteBook": "[note-book]",
      "NoteFiles": "[note-files]",
      "NoteInternals": "[note-internals]",
      "NoteInternalsPfx": "0",
      "NoteItemsPfx": "",
      "JoinStr": "-"
    },
    "NoteDirNameIdxes": {
      "MinIdx": 1,
      "MaxIdx": 999,
      "IncIdx": true,
      "FillGapsByDefault": false,
      "IdxFmt": "D3"
    },
    "NoteInternalDirNameIdxes": {
      "MinIdx": 1,
      "MaxIdx": 999,
      "IncIdx": true,
      "FillGapsByDefault": true,
      "IdxFmt": "D1"
    },
    "FileNames": {
      "NoteBookJsonFileName": "[note-book].json",
      "NoteItemJsonFileName": "[note].json",
      "NoteItemMdFileName": "[note].md",
      "PrependTitleToNoteMdFileName": true,
      "KeepFileName": ".keep"
    },
    "FileContents": {
      "KeepFileContentsTemplate": "{0}",
      "NoteFileContentsTemplate": "<input type=\"hidden\" name=\"{2}\" value=\"{1}\" />\n\n# {0}  \n\n",
      "RequireTrmrkGuidInNoteJsonFile": true,
      "RequireTrmrkGuidInNoteMdFile": false
    }
  }
}
