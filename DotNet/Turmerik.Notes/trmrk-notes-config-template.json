{
  "IsDevEnv": false,
  "RequiredClientVersion": 1,
  "ClientRedirectUrl": null,
  "NoteDirPairs": {
    "FileNameMaxLength": 100,
    // "SerializeToJson": true,
    "TrmrkGuidInputName": "trmrk_guid",
    "ArgOpts": {
      "WorkDir": "wd",
      "SortIdx": "x",
      "OpenMdFile": "o",
      "CreateNoteBookDirsPair": "b",
      "CreateNoteFilesDirsPair": "f",
      "CreateNoteInternalDirsPair": "i"
    },
    "DirNames": {
      "NoteBook": "[note-book]",
      "NoteFiles": "[note-files]",
      "NoteInternals": "[note-internals]",
      "NoteInternalsPfx": "0",
      "NoteItemsPfx": "",
      "JoinStr": "-"
    },
    "NoteDirNameIdxes": {
      "MinIdx": 1,
      "MaxIdx": 999,
      "IncIdx": true,
      "FillGapsByDefault": false,
      "IdxFmt": "D3"
    },
    "NoteInternalDirNameIdxes": {
      "MinIdx": 1,
      "MaxIdx": 999,
      "IncIdx": true,
      "FillGapsByDefault": true,
      "IdxFmt": "D1"
    },
    "FileNames": {
      "NoteBookJsonFileName": "[note-book].json",
      "NoteItemJsonFileName": "[note].json",
      "NoteItemMdFileName": "[note].md",
      "PrependTitleToNoteMdFileName": true,
      "KeepFileName": ".keep"
    },
    "FileContents": {
      "KeepFileContentsTemplate": "{0}",
      "NoteFileContentsTemplate": "<input type=\"hidden\" name=\"{2}\" value=\"{1}\" />\n\n# {0}  \n\n",
      "RequireTrmrkGuidInNoteJsonFile": true,
      "RequireTrmrkGuidInNoteMdFile": false
    }
  }
}
