{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["node", "vite/client"],
    "noEmit": true,
    "isolatedModules": true,
    "lib": [
      "dom",
      "esnext"
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
